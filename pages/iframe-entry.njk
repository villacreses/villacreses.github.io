---
collection: iframe-proxies
pagination:
  data: iframe-proxies
  size: 1
  alias: proxy
permalink: "{{proxy.permalink}}"
---
<!doctype html>
<html lang="en">
<head>
  <title>{{proxy.title}}</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta name="robots" content="noindex, nofollow">
</head>
<body>
  <iframe 
  title="{{proxy.title}}"
  src="{{proxy.src}}"
  referrerpolicy="no-referrer"
></iframe>
<style>
  html, body {
    overflow: hidden;
    margin: 0;
    padding: 0;
  }
  iframe {
    padding: 0;
    margin-top: -2px;
    margin-left: -2px;
    height: 100vh;
    width: 100vw;
  }
</style>
</body>
</html>