---
layout: layouts/post
collection: notes
pagination:
  data: notes
  size: 1
  alias: note
permalink: "/writing/notes/{{ note.slug }}.html"
eleventyComputed:
  title: "{{ note.frontmatter.title }}"
  date: "{{ note.frontmatter.date }}"
  readingTime: "{{ note.content | wordcount }}"
---
{{note.content | markdown | safe }}
